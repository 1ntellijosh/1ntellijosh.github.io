{
  "name": "starfighter",
  "version": "1.0.0",
  "description": "StarFighter game converted to TypeScript",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:watch": "vite build --watch",
    "type-check": "tsc --noEmit",
    "type-check:watch": "tsc --noEmit --watch",
    "preview": "vite preview",
    "start": "node server.js",
    "start:prod": "npm run build && node server.js"
  },
  "dependencies": {
    "express": "^4.18.2",
    "react": "^19.2.1",
    "react-dom": "^19.2.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/jquery": "^3.5.29",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.2",
    "typescript": "^5.3.3",
    "vite": "^7.2.7"
  }
}
